{
    "name": "anime-db/cache-time-keeper-bundle",
    "license": "MIT",
    "type": "library",
    "description": "Cache time keeper",
    "keywords": ["php", "cache", "shmop"],
    "homepage": "http://github.com/anime-db/cache-time-keeper-bundle",
    "version": "1.0.2",
    "autoload": {
        "psr-4": {
            "AnimeDb\\Bundle\\CacheTimeKeeperBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AnimeDb\\Bundle\\CacheTimeKeeperBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.4.0",
        "anime-db/shmop": ">=1.0"
    },
    "require-dev": {
        "symfony/dependency-injection": "2.3.*",
        "symfony/config": "2.6.*",
        "symfony/http-kernel": "2.6.*",
        "symfony/console": "2.6.*",
        "doctrine/orm": ">=2.2.3,<2.4-dev"
    },
    "minimum-stability": "dev"
}
