{
    "name": "anime-db/cache-time-keeper-bundle",
    "license": "MIT",
    "type": "library",
    "description": "Cache time keeper",
    "keywords": ["php", "cache", "shmop"],
    "homepage": "http://github.com/anime-db/cache-time-keeper-bundle",
    "autoload": {
        "psr-4": {
            "AnimeDb\\Bundle\\CacheTimeKeeperBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AnimeDb\\Bundle\\CacheTimeKeeperBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.5.0",
        "symfony/symfony": "~2.3|~3.0",
        "doctrine/orm": "~2.4|~2.5",
        "doctrine/doctrine-bundle": "~1.4"
    },
    "require-dev": {
        "anime-db/shmop": "1.0.*",
        "phpunit/phpunit": "4.8.*",
        "scrutinizer/ocular": "1.3.*",
        "satooshi/php-coveralls": "^1.0"
    },
    "suggest": {
        "ext-memcache": "Allows use a memcache cache driver",
        "anime-db/shmop": "Allows use a shmop cache driver"
    },
    "minimum-stability": "stable"
}
